@model List<AdoptameLiberia.Models.Campanias.InscripcionCastracion>

<h2>Animales Inscritos</h2>

<table class="table">
    <tr>
        <th>Animal</th>
        <th>Veterinario</th>
        <th>Resultado</th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Animal.Nombre_Animal</td>
            <td>@item.VeterinarioAsignado</td>
            <td>@item.Resultado</td>
            <td>
                @Html.ActionLink("Asignar Veterinario", "AsignarVeterinario",new { id = item.Id },new { @class = "btn btn-primary btn-sm" })
            </td>
        </tr>
    }
</table>